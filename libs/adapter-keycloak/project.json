{
  "name": "adapter-keycloak",
  "projectType": "library",
  "root": "libs/adapter-keycloak",
  "sourceRoot": "libs/adapter-keycloak/src",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/adapter-keycloak",
        "tsConfig": "libs/adapter-keycloak/tsconfig.lib.json",
        "main": "libs/adapter-keycloak/src/index.ts",
        "assets": [
          "libs/adapter-keycloak/README.md",
          {
            "input": "libs/adapter-keycloak",
            "glob": "package.json",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [],
      "options": {
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "scope:auth-ws",
    "type:adapter"
  ],
  "lint": {
    "executor": "@nx/eslint:lint",
    "options": {
      "lintFilePatterns": [
        "libs/adapter-keycloak/**/*.ts",
        "libs/adapter-keycloak/**/*.tsx"
      ]
    }
  }
}