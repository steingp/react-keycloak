{
  "name": "@auth-ws/adapter-keycloak",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "nx build adapter-keycloak",
    "test": "nx test adapter-keycloak"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "/dist/libs/adapter-keycloak",
          "tsConfig": "./libs/adapter-keycloak/tsconfig.lib.json",
          "main": "./libs/adapter-keycloak/src/index.ts"
        }
      },
      "test": {
        "executor": "@nx/vite:test",
        "outputs": [],
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^20.19.7",
    "@vitejs/plugin-react": "^4.6.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}