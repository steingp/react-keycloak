{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "types": [
      "node",
      "vite/globals",
      "vite",
      "vitest",
      "vitest/importMeta",
      "@nx/react/typings/cssmodule",
      "@nx/react/typings/image"
    ],
    "paths": {
      "@auth-ws/adapter-keycloak": ["libs/adapter-keycloak/src/index.ts"],
      "@auth-ws/adapter-mock": ["libs/adapter-mock/src/index.ts"],
      "@auth-ws/api-graphql": ["libs/api-graphql/src/index.ts"],
      "@auth-ws/core-domain": ["libs/core-domain/src/index.ts"],
      "@auth-ws/ui-react": ["libs/ui-react/src/index.ts"]
    }
  }
}
