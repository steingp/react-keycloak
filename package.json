{
  "name": "auth-ws",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "run:dev": "pnpm exec nx serve auth-demo",
    "release:patch": "pnpm bump:patch && pnpm pack:auth",
    "release:minor": "pnpm bump:minor && pnpm pack:auth",
    "release:major": "pnpm bump:major && pnpm pack:auth",
    "bump:patch": "pnpm -w @auth-ws/adapter-keycloak version patch",
    "bump:minor": "pnpm -w @auth-ws/adapter-keycloak version minor",
    "bump:major": "pnpm -w @auth-ws/adapter-keycloak version major",
    "pack:auth-cloak": "cd libs/adapter-keycloak && pnpm pack",
    "install:keycloak:latest": "node libs/adapter-keycloak/install-latest-adapter-keycloak.mjs",
    "test:e2e": "pnpm exec nx reset && pnpm exec nx e2e auth-demo-e2e",
    "test:watch": "vitest --watch",
    "clean:tgz": "rimraf --glob \"libs/adapter-keycloak/*.tgz\"",
    "clean:npm": "rm -rf node_modules pnpm-lock.yaml & pnpm exec nx reset & pnpm install",
    "postinstall": "playwright install",
    "validate-project-paths": "node tools/validate-projects/validate-project-paths.cjs"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.36.0",
    "@nx/devkit": "21.2.2",
    "@nx/eslint": "21.2.2",
    "@nx/eslint-plugin": "21.2.2",
    "@nx/jest": "^21.5.3",
    "@nx/js": "21.2.2",
    "@nx/playwright": "21.2.2",
    "@nx/react": "21.2.2",
    "@nx/vite": "21.2.2",
    "@nx/web": "21.2.2",
    "@nx/workspace": "21.2.2",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.6",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.55.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.0",
    "verdaccio": "^6.0.5",
    "vite": "6.3.6",
    "vitest": "3.2.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "6.3.6",
      "@vitejs/plugin-react": "4.7.0",
      "rollup": "4.24.0",
      "@types/node": "22.18.6"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "nx",
      "unrs-resolver"
    ]
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}