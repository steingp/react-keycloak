{
  "name": "@auth-ws/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "run:dev": "pnpm exec nx serve auth-demo",
    "release:patch": "pnpm bump:patch && pnpm pack:auth",
    "release:minor": "pnpm bump:minor && pnpm pack:auth",
    "release:major": "pnpm bump:major && pnpm pack:auth",
    "bump:patch": "pnpm -w @auth-ws/auth-keycloak version patch",
    "bump:minor": "pnpm -w @auth-ws/auth-keycloak version minor",
    "bump:major": "pnpm -w @auth-ws/auth-keycloak version major",
    "pack:auth-cloak": "cd libs/auth-keycloak && pnpm pack",
    "install:keycloak:latest": "node libs/auth-keycloak/install-latest-auth-keycloak.mjs",
    "test:e2e": "pnpm exec nx reset && pnpm exec nx e2e auth-demo-e2e",
    "test:watch": "vitest --watch",
    "clean:tgz": "rimraf --glob \"libs/auth-keycloak/*.tgz\"",
    "clean:npm": "rm -rf node_modules pnpm-lock.yaml & pnpm exec nx reset & pnpm install",
    "postinstall": "playwright install"
  },
  "private": true,
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "21.2.2",
    "@nx/eslint": "21.2.2",
    "@nx/eslint-plugin": "21.2.2",
    "@nx/js": "21.2.2",
    "@nx/playwright": "21.2.2",
    "@nx/react": "21.2.2",
    "@nx/vite": "21.2.2",
    "@nx/web": "21.2.2",
    "@nx/workspace": "21.2.2",
    "@playwright/test": "^1.36.0",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.2.2",
    "prettier": "^2.6.2",
    "sass": "^1.55.0",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  }
}
